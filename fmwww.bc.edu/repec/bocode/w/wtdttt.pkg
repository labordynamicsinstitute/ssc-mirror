f wtdtttprob.ado
f wtdtttprob.sthlp
f wtdpreddur.ado
f ../m/mlwtd_exp.ado
f ../m/mlwtd_lnorm.ado
f ../m/mlwtd_wei.ado
f wtdttt_ex.do
f wtddat_covar.dta
