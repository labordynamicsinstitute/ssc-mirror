PROGRAM command
BEGIN
  put "eurostatuse "
  put main.seriename
    beginoptions
      option main.ck_clear
      option main.ck_long
      option main.ck_nolabel
      option main.ck_noflags
      option main.ck_noerase
      option main.ck_uncompressed
      option main.ck_save
      put " geo("main.country
      put ") "
      put " keepdim("main.dimension3
      put ") "
      put " start("main.start
      put ") "
      put " end("main.end
      put ")
    endoptions
END
