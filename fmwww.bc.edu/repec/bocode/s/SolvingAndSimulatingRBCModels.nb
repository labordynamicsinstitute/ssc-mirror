Cell[BoxData[
 RowBox[{
  RowBox[{"hPfilterz", "[", 
   RowBox[{"data_List", ",", 
    RowBox[{"\[Lambda]_", "?", "Positive"}]}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "totime", ",", "trend", ",", "cycle", ",", "KKMAT", ",", "IDM", ",", 
      "MATA"}], "}"}], ",", 
    RowBox[{
     RowBox[{"totime", "=", 
      RowBox[{"Length", "[", "data", "]"}]}], ";", 
     RowBox[{"KKMAT", "=", 
      RowBox[{"SparseArray", "[", "\n", "    ", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{
           RowBox[{"{", 
            RowBox[{"r_", ",", " ", "r_"}], "}"}], " ", "->", " ", "1."}], 
          ",", 
          RowBox[{
           RowBox[{
            RowBox[{"{", 
             RowBox[{"r_", ",", " ", "s_"}], "}"}], " ", "/;", " ", 
            RowBox[{"s", " ", "==", " ", 
             RowBox[{"r", " ", "+", " ", "1"}]}]}], " ", "->", " ", 
           RowBox[{"-", "2."}]}], ",", 
          RowBox[{
           RowBox[{
            RowBox[{"{", 
             RowBox[{"r_", ",", " ", "s_"}], "}"}], " ", "/;", " ", 
            RowBox[{"s", " ", "==", " ", 
             RowBox[{"r", " ", "+", " ", "2"}]}]}], " ", "->", " ", "1."}]}], 
         "}"}], ",", " ", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"totime", " ", "-", " ", "2"}], ")"}], ",", " ", 
          "totime"}], "}"}], ",", "0."}], "]"}]}], ";", 
     RowBox[{"IDM", "=", 
      RowBox[{"IdentityMatrix", "[", "totime", "]"}]}], ";", 
     RowBox[{"MATA", "=", 
      RowBox[{"IDM", "+", 
       RowBox[{"\[Lambda]", " ", 
        RowBox[{
         RowBox[{"Transpose", "[", "KKMAT", "]"}], ".", "KKMAT"}]}]}]}], " ", 
     ";", 
     RowBox[{"trend", " ", "=", " ", 
      RowBox[{"LinearSolve", "[", 
       RowBox[{"MATA", ",", " ", "data", ",", 
        RowBox[{"Method", "\[Rule]", "\"\<Automatic\>\""}]}], "]"}]}], ";", 
     RowBox[{"cycle", "=", 
      RowBox[{"data", "-", "trend"}]}], ";", 
     RowBox[{"{", 
      RowBox[{"cycle", ",", "trend"}], "}"}]}]}], "]"}]}]], "Input",
